@article{magnussonDistributedApproachOptimal2015,
  title = {A {{Distributed Approach}} for the {{Optimal Power-Flow Problem Based}} on {{ADMM}} and {{Sequential Convex Approximations}}},
  author = {Magn{\'u}sson, Sindri and Weeraddana, Pradeep Chathuranga and Fischione, Carlo},
