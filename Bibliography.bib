}

@article{quezadalarrivaImpactQuitoFirst2023,
  title = {Impact of {{Quito}}'s First Metro Line on the Accessibility to Urban Opportunities},
  author = {Quezada Larriva, Adriana and Orellana, Daniel and Guerrero Balarezo, Mar{\'i}a Laura and Garc{\'i}a, Javier Andr{\'e}s and C{\'a}rdenas Villenas, Galo and Osorio Guerrero, Pablo},
