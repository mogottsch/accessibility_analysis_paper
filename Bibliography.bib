@article{knapCompositeXminuteCity2023,
  title = {A Composite {{X-minute}} City Cycling Accessibility Metric and Its Role in Assessing Spatial and Socioeconomic Inequalities \textendash{} {{A}} Case Study in {{Utrecht}}, the {{Netherlands}}},
  author = {Knap, Elizabeth and Ulak, Mehmet Baran and Geurs, Karst T. and Mulders, Alex and {van der Drift}, Sander},
