@article{krieglerPathwaysLimitingWarming2018,
  title = {Pathways Limiting Warming to 1.5\textdegree{{C}}: A Tale of Turning around in No Time?},
  shorttitle = {Pathways Limiting Warming to 1.5\textdegree{{C}}},
  author = {Kriegler, Elmar and Luderer, Gunnar and Bauer, Nico and Baumstark, Lavinia and Fujimori, Shinichiro and Popp, Alexander and Rogelj, Joeri and Strefler, Jessica and {van Vuuren}, Detlef P.},
  year = {2018},
  month = apr,
  journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
  volume = {376},
  number = {2119},
  pages = {20160457},
  publisher = {{Royal Society}},
  doi = {10.1098/rsta.2016.0457},
  urldate = {2023-11-06},
  abstract = {We explore the feasibility of limiting global warming to 1.5\textdegree C without overshoot and without the deployment of carbon dioxide removal (CDR) technologies. For this purpose, we perform a sensitivity analysis of four generic emissions reduction measures to identify a lower bound on future CO2 emissions from fossil fuel combustion and industrial processes. Final energy demand reductions and electrification of energy end uses as well as decarbonization of electricity and non-electric energy supply are all considered. We find the lower bound of cumulative fossil fuel and industry CO2 emissions to be 570 GtCO2 for the period 2016\textendash 2100, around 250 GtCO2 lower than the lower end of available 1.5\textdegree C mitigation pathways generated with integrated assessment models. Estimates of 1.5\textdegree C-consistent CO2 budgets are highly uncertain and range between 100 and 900 GtCO2 from 2016 onwards. Based on our sensitivity analysis, limiting warming to 1.5\textdegree C will require CDR or terrestrial net carbon uptake if 1.5\textdegree C-consistent budgets are smaller than 650 GtCO2. The earlier CDR is deployed, the more it neutralizes post-2020 emissions rather than producing net negative emissions. Nevertheless, if the 1.5\textdegree C budget is smaller than 550 GtCO2, temporary overshoot of the 1.5\textdegree C limit becomes unavoidable if CDR cannot be ramped up faster than to 4 GtCO2 in 2040 and 10 GtCO2 in 2050. This article is part of the theme issue `The Paris Agreement: understanding the physical and social challenges for a warming world of 1.5\textdegree C above pre-industrial levels'.},
  keywords = {1.5\textdegree C goal,carbon budget,carbon dioxide removal,CO\-2 emissions,integrated assessment,mitigation pathways},
  file = {/home/moritz/Zotero/storage/4G3IMUNG/Kriegler et al. - 2018 - Pathways limiting warming to 1.5°C a tale of turn.pdf;/home/moritz/Zotero/storage/Q8EKPBVG/pathways-limiting-warming-to-15c-a-tale-of-turning-around-in-no--2018.pdf.pdf}
}

@inproceedings{krismerComputingIsochronesMultimodal2017,
  title = {Computing {{Isochrones}} in {{Multimodal Spatial Networks}} Using {{Tile Regions}}},
  booktitle = {Proceedings of the 29th {{International Conference}} on {{Scientific}} and {{Statistical Database Management}}},
  author = {Krismer, Nikolaus and Silbernagl, Doris and Specht, G{\"u}nther and Gamper, Johann},
  year = {2017},
  month = jun,
  series = {{{SSDBM}} '17},
  pages = {1--6},
  publisher = {{Association for Computing Machinery}},
  address = {{New York, NY, USA}},
  doi = {10.1145/3085504.3085538},
  urldate = {2023-05-31},
  abstract = {This paper describes a new method to compute isochrones in multimodal spatial networks, which aims at finding a good trade-off between memory usage and runtime. In the past, approaches based on Dijkstra's algorithm have been proposed. For small networks, the entire network is first loaded in main memory, where the network is expanded to determine the isochrone. For large networks that do not fit in main memory, approaches that load the network vertex-by-vertex during the expansion phase have been proposed. They keep the memory footprint minimal, but have to query the database for each node in the isochrone, which can be very time consuming. The method presented in this paper uses tiles (which are well known from interactive online maps) to realize chunk-loading of vertices by utilizing so-called tile regions. This approach significantly reduces the number of database requests, while keeping the memory usage low. Our method is able to compute isochrones even in large networks at a reasonable time. An experimental evaluation shows that the new algorithm clearly outperforms previous competitive approaches such as MINE and MINEX.},
  isbn = {978-1-4503-5282-6},
  keywords = {algorithms,isochrone,spatial database},
  file = {/home/moritz/Zotero/storage/AZM5LT6E/krismer2017.pdf.pdf;/home/moritz/Zotero/storage/RTYS8DSI/Krismer et al. - 2017 - Computing Isochrones in Multimodal Spatial Network.pdf}
}

